# Lavalink Server Configuration

# Server Basic Settings
LAVALINK_PASSWORD=your_super_secret_password_123!
SERVER_PORT=2333
SERVER_ADDRESS=0.0.0.0

# Source Configurations
YOUTUBE_SOURCE_ENABLED=false
SOUNDCLOUD_SOURCE_ENABLED=true
TWITCH_SOURCE_ENABLED=true
BANDCAMP_SOURCE_ENABLED=true
VIMEO_SOURCE_ENABLED=true
HTTP_SOURCE_ENABLED=true

# Performance Tuning
BUFFER_DURATION_MS=500
FRAME_BUFFER_DURATION_MS=6000
OPUS_ENCODING_QUALITY=8
RESAMPLING_QUALITY=MEDIUM

# Logging Configuration
ROOT_LOG_LEVEL=INFO
LAVALINK_LOG_LEVEL=DEBUG
LOGGING_FILE_PATH=./custom-logs/
LOG_MAX_FILE_SIZE=2GB
LOG_MAX_HISTORY=45

# Metrics and Monitoring
PROMETHEUS_METRICS_ENABLED=true
PROMETHEUS_METRICS_ENDPOINT=/prometheus-metrics

# Filter Configurations
FILTER_VOLUME_ENABLED=true
FILTER_EQUALIZER_ENABLED=true
FILTER_KARAOKE_ENABLED=true
FILTER_TIMESCALE_ENABLED=true

# Advanced Settings
TRACK_STUCK_THRESHOLD_MS=15000
PLAYER_UPDATE_INTERVAL=7
GC_WARNINGS_ENABLED=true